  "dependencies": {
    "@harbour-enterprises/super-editor": "*",
    "playwright-core": "^1.48.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/superdoc.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/superdoc/issues"
  },
  "homepage": "https://github.com/your-org/superdoc#readme"
}
