{
  "name": "@superdoc/playwright-runtime",
  "version": "0.1.0",
  "description": "Playwright-based headless runtime for SuperDoc document operations",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
