{
  "name": "@harbour-enterprises/superdoc-vue2",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/superdoc-vue2.es.js",
      "require": "./dist/superdoc-vue2.cjs.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/superdoc-vue2.cjs.js",
  "module": "./dist/superdoc-vue2.es.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/ --ext .vue,.js --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "check": "prettier --check src/",
    "test": "NODE_ENV=test vitest",
    "test:ci": "NODE_ENV=test vitest --silent"
  },
  "devDependencies": {
    "@harbour-enterprises/superdoc": "^1.0.1",
    "@vitejs/plugin-vue2": "^2.3.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.31",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.4",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-vue2": "^2.0.3",
    "vue": "^2.6.13"
  },
  "peerDependencies": {
    "@harbour-enterprises/superdoc": "^1.0.1",
    "vue": "^2.6.13"
  },
  "repository": "https://github.com/Harbour-Enterprises/Superdoc.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
