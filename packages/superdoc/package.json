  },
  "types": "./dist/packages/superdoc/src/index.d.ts",
  "main": "./dist/superdoc.umd.js",
  "module": "./dist/superdoc.es.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && npm run build:umd",
    "postbuild": "node ./scripts/ensure-types.cjs",
    "build:es": "vite build",
    "watch:es": "nodemon --watch src --ext js,ts,vue --exec \"npm run build:es\" --delay 100ms",
    "build:umd": "vite build --config vite.config.umd.js",
    "clean": "rm -rf dist",
    "pack:local": "npm run build:es && npm pack && mv $(ls superdoc-*.tgz) ./superdoc.tgz",
    "pack": "npm run build:es && npm pack && mv $(ls superdoc-*.tgz) ./superdoc.tgz",
    "test": "vitest",
    "test:debug": "vitest --inspect-brk --pool threads --poolOptions.threads.singleThread"
  },
